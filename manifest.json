{
  "name": "mediamarkt",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "mediamarkt",
  "permissions": [
    "declarativeContent", 
    "storage",
    "webRequest",
    "webRequestBlocking",
    ":///"
  ],
  "content_security_policy": "default-src 'self';",
  "content_scripts": [
    {
      "matches": ["https://www.mediamarkt.com.tr/"],
      "js": ["main.js"],
      "css": ["main.css"],
      "all_frames": true
    }
  ]

}
